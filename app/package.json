  "private": true,
  "dependencies": {
    "axios": "^0.19.2",
    "components": "^0.1.0",
    "contexts": "^0.0.0",
    "dotenv": "^8.2.0",
    "parcel-bundler": "^1.12.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "recompose": "^0.30.0",
    "styled-components": "^5.0.1",
